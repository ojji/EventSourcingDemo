{
    "name": "boardweb",
    "version": "1.0.0",
    "main": "webpack.config.js",
    "license": "MIT",
    "scripts": {
        "start": "yarn run webpack-prod && cross-env ASPNETCORE_ENVIRONMENT=Production dotnet run",
        "start-dev": "concurrently \"yarn run webpack-watch-dev\" \"yarn run dotnet-watch\"",
        "dotnet-watch": "cross-env ASPNETCORE_ENVIRONMENT=Development dotnet watch run",
        "webpack-dev": "cross-env NODE_ENV=development webpack --color --progress --bail",
        "webpack-prod": "cross-env NODE_ENV=production webpack --color --progress --bail",
        "webpack-watch-dev": "cross-env NODE_ENV=development webpack --color --watch --progress"
    },
    "dependencies": {
        "@angular/common": "^4.1.3",
        "@angular/compiler": "^4.1.3",
        "@angular/compiler-cli": "^4.1.3",
        "@angular/core": "^4.1.3",
        "@angular/forms": "^4.1.3",
        "@angular/http": "^4.1.3",
        "@angular/platform-browser": "^4.1.3",
        "@angular/platform-browser-dynamic": "^4.1.3",
        "@angular/router": "^4.1.3",
        "@types/requirejs": "^2.1.29",
        "angular2-moment": "^1.3.3",
        "core-js": "^2.4.1",
        "font-awesome": "^4.7.0",
        "foundation-sites": "^6.3.1",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.4.0",
        "ts-helpers": "^1.1.2",
        "zone.js": "^0.8.11"
    },
    "devDependencies": {
        "@ngtools/webpack": "^1.3.3",
        "angular2-template-loader": "^0.6.2",
        "awesome-typescript-loader": "^3.1.3",
        "clean-webpack-plugin": "^0.1.16",
        "concurrently": "^3.4.0",
        "copy-webpack-plugin": "^4.0.1",
        "cross-env": "^5.0.0",
        "css-loader": "^0.28.4",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "^0.11.1",
        "html-webpack-plugin": "^2.28.0",
        "node-sass": "^4.5.3",
        "postcss-loader": "^2.0.5",
        "raw-loader": "^0.5.1",
        "sass-loader": "^6.0.5",
        "style-loader": "^0.18.1",
        "typescript": "^2.3.4",
        "webpack": "^2.6.1",
        "webpack-merge": "^4.1.0"
    }
}
